  <!--登录日志高级查询-->
  <option key="getqueryloginlog">
    <values>
      <value>
        SELECT * FROM
        (
        SELECT A.*, ROWNUM RN
        FROM (SELECT * FROM angel_sys_loginlog order by logintime desc ) A
        WHERE ROWNUM &lt;= {0}*{1}
